.eqv SEVENSEG_LEFT 0xFFFF0011	#Dia chi cua den LED 7 doan ben trai
.eqv SEVENSEG_RIGHT 0xFFFF0010	#Dia chi cua den LED 7 doan ben phai
.eqv IN_ADDRESS_HEXA_KEYBOARD	0xFFFF0012	#Dia chi nhap vao cua keyboard 16
.eqv OUT_ADDRESS_HEXA_KEYBOARD	0xFFFF0014	#Dia chi hien thi cua keyboard 16

.data
#Ma hoa Digital Lab Sim cho de su dung
zero:	.byte 0x3f
one:	.byte 0x6
two:	.byte 0x5b
three:	.byte 0x4f
four:	.byte 0x66
five:	.byte 0x6d
six:	.byte 0x7d
seven:	.byte 0x7
eight:	.byte 0x7f
nine:	.byte 0x6f

mess1: .asciiz	"Ket qua khong ton tai\n"
mess2: .asciiz	"Ban hay nhap 2 toan hang" 
.text
main:

Initialize:
	li $t0, SEVENSEG_LEFT	#Bien gia tri so cua den LED trai
	li $t5, SEVENSEG_RIGHT 	#Bien gia tri so cua den LED phai
	li $s0, 0		#Bien kiem tra bien nhap vao: (0: so), (1: toan tu)
	li $s1, 0		#So hien thi o led phai
	li $s2, 0		#So hien thi o led trai
	li $s3, 0		#Bien kiem tra loai toan tu (1: +, 2: -, 3: *, 4: /, 5: %)
	li $s4, 0		#So thu nhat
	li $s5, 0		#So thu hai
	li $s6, 0		#Ket qua phep tinh: +, -, *, /, %
	li $t9, 0		#Gia tri tam thoi
	
	li $t1, IN_ADDRESS_HEXA_KEYBOARD	#Bien dieu khien hang keyboard va cho phep keyboard ngat
	li $t2, OUT_ADDRESS_HEXA_KEYBOARD	#Bien chua vi tri key nhap vao hang va cot
	li $t3, 0x80				#Bit dung de cho phep ngat keyboard va cho phep kiem tra tung hang keyboard
	sb $t3, 0($t1)
	li $t7, 0		#Gia tri cua so hien thi tren led
	li $t4, 0		#byte hien thi tren led, zero->nine

First_value:
	li 	$t7, 0		#Gia tri cua bit can hien thi ban dau
	addi	$sp, $sp, 4	#Day vao trong stack
	sb 	$t7, 0($sp)	
	lb	$t4, zero	#Bit dau tien can hien thi
	addi 	$sp, $sp, 4	#Day vao trong stack
	sb	$t4, 0($sp)
	
Loop1:
	nop
	nop
	nop
	nop
	b	Loop1		#Doi ngat
	nop
	nop
	nop
	nop
	b	Loop1		#Doi ngat
	nop
	nop
	nop
	nop
	b	Loop1		#Doi ngat
end_loop1:

end_main:
	li	$v0, 10
	syscall
	

#Xu li khi xay ra interupt
#Hien thi so vua bam len den LED 7
#Kiem tra tung hang xem co dc bam ko

	
.ktext 0x80000180

	jal check_row1			#Kiem tra hang 1 xem co phim nao dc nhap ko?
	bnez $t3, convert_row1		#t3 != 0 -> co phim dc nhap, kiem tra cac phim trong hang, lay no ra
	nop
	
	jal check_row2			#TTu
	bnez $t3, convert_row2		
	nop
	
	jal check_row3			#TTu
	bnez $t3, convert_row3		
	nop
	
	jal check_row4			#TTu
	bnez $t3, convert_row4		
	
	

#Kiem tra tung hang mot xem phim duoc nhap o hang nao

check_row1:
	addi	$sp, $sp, 4
	sw	$ra, 0($sp)		#Luu lai vi ve sau co the doi
	li	$t3, 0x81		#Kich hoat interupt, cho phem ban phim o hang 1
	sb	$t3, 0($t1)
	jal	Get_value		#Lay vi tri cua phim dc nhap (neu co)
	lw	$ra, 0($sp)		
	addi	$sp, $sp, -4
	jr 	$ra
	
check_row2:
	addi	$sp, $sp, 4
	sw	$ra, 0($sp)		#Luu lai vi ve sau co the doi
	li	$t3, 0x82		#Kich hoat interupt, cho phep ban phim o hang 2
	sb	$t3, 0($t1)
	jal	Get_value		#Lay vi tri cua phim dc nhap (neu co)
	lw	$ra, 0($sp)		
	addi	$sp, $sp, -4
	jr 	$ra

check_row3:
	addi	$sp, $sp, 4
	sw	$ra, 0($sp)		#Luu lai vi ve sau co the doi
	li	$t3, 0x84		#Kich hoat interupt, cho phep ban phim o hang 3
	sb	$t3, 0($t1)
	jal	Get_value		#Lay vi tri cua phim dc nhap (neu co)
	lw	$ra, 0($sp)		
	addi	$sp, $sp, -4
	jr 	$ra
	
check_row4:
	addi	$sp, $sp, 4
	sw	$ra, 0($sp)		#Luu lai vi ve sau co the doi
	li	$t3, 0x88		#Kich hoat interupt, cho phep ban phim o hang 4
	sb	$t3, 0($t1)
	jal	Get_value		#Lay vi tri cua phim dc nhap (neu co)
	lw	$ra, 0($sp)		
	addi	$sp, $sp, -4
	jr 	$ra	



#Lay gia tri cua phim vua dc nhap

Get_value:
	addi	$sp, $sp, 4
	sw	$ra, 0($sp)
	li	$t2, OUT_ADDRESS_HEXA_KEYBOARD	#Dia chi phim duoc nhap
	lb	$t3, 0($t2)			#load vi tri phim dc nhap
	lw	$ra, 0($sp)
	addi	$sp, $sp, -4
	jr	$ra
	

#Convert tu vi tri sang bit
#------------------------------------
#Convert hang 1: co 4 gia tri: 0, 1, 2, 3
#Ma hoa tuong ung: zero:	.byte 0x3f
#		   one:		.byte 0x6
#		   two:		.byte 0x5b
#		   three:	.byte 0x4f
#------------------------------------

convert_row1:
	beq	$t3, 0x11, case_0		#0x11	-> so 0
	beq	$t3, 0x21, case_1		#0x21	-> so 1
	beq	$t3, 0x41, case_2		#0x41 	-> so 2
	beq	$t3, 0xffffff81, case_3		#0xffffff81 -> so 3
case_0:
	lb	$t4, zero	#t4 = zero (Ma hoa cua '0' tren Digital Lab Sim)
	li	$t7, 0		#t7 = 0
	j 	update
case_1:
	lb	$t4, one	#TTu
	li	$t7, 1
	j	update
case_2:
	lb	$t4, two	#TTu
	li	$t7, 2
	j	update
case_3:
	lb	$t4, three	#TTu
	li	$t7, 3
	j	update
	
#------------------------------------
#Convert hang 2: co 4 gia tri: 4, 5, 6, 7
#Ma hoa tuong ung: four:	.byte 0x66
#		   five:	.byte 0x6d
#		   six:		.byte 0x7d
#		   seven:	.byte 0x7
#------------------------------------

convert_row2:
	beq	$t3, 0x12, case_4		#0x12 -> so 4
	beq	$t3, 0x22, case_5		#0x22 -> so 5
	beq	$t3, 0x42, case_6		#0x42 -> so 6
	beq	$t3, 0xffffff82, case_7		#0xffffff82 -> so 7
case_4:
	lb	$t4, four	#t4 = four (Ma hoa cua '4' tren Digital Lab Sim)
	li	$t7, 4		#t7 = 4
	j 	update
case_5:
	lb	$t4, five	#TTu
	li	$t7, 5
	j	update
case_6:
	lb	$t4, six	#TTu
	li	$t7, 6
	j	update
case_7:
	lb	$t4, seven	#TTu
	li	$t7, 7
	j	update

#------------------------------------
#Convert hang 3: co 4 gia tri: 8, 9, a, b 
#Ma hoa tuong ung: eight:	.byte 0x7f
#		   nine:	.byte 0x6f
#------------------------------------

convert_row3:
	beq	$t3, 0x14, case_8
	beq	$t3, 0x24, case_9
	beq	$t3, 0x44, case_a
	beq	$t3, 0xffffff84, case_b
case_8:
	lb	$t4, eight
	li	$t7, 8
	j	update
case_9:
	lb	$t4, nine
	li	$t7, 9
	j	update


#Truong hop phim a (phep cong)

case_a:
	addi	$s0, $s0, 1		#s0 = 1 -> toan tu dc nhap vao
	addi	$s3, $zero, 1		#s3 = 1 -> phep cong
	
	j	set_first_number	#Chuyen den function chuyen 2 byte dang hien tren 2 LED thanh so de tinh toan
	

#Truong hop phim a (phep tru)

case_b:
	addi	$s0, $s0, 1		#s0 = 1 -> toan tu dc nhap vao
	addi	$s3, $zero, 2		#s3 = 2 -> phep tru
	j	set_first_number	#Chuyen den function chuyen 2 byte dang hien tren 2 LED thanh so		
			

#Convert hang 3: co 4 gia tri: c, d, e, f 				


convert_row4:
	beq	$t3, 0x18, case_c	
	beq	$t3, 0x28, case_d
	beq	$t3, 0x48, case_e
	beq	$t3, 0xffffff88, case_f
	

#Truong hop phim c (phep nhan)

case_c:

	addi	$s0, $s0, 1		#s0 = 1 -> toan tu duoc nhap vao
	addi	$s3, $zero, 3		#s3 = 3 -> phep nhan
	
	j set_first_number		#Chuyen den function chuyen 2 byte dang 2 led thanh so de tinh toan
	

#Truong hop phim d (phep chia)
																												
case_d:
	addi	$s0, $s0, 1		#s0 = 1 -> toan tu dc nhap vao
	addi	$s3, $zero, 4		#S3 = 4 -> phep chia lay nguyen
	j set_first_number
	

#Truong hop phim d (phep chia lay du %)

case_e:
	addi	$s0, $s0, 1		#s0 = 1 -> toan tu duoc nhap vao
	addi	$s3, $zero, 5		#s3 = 5 -> chia lay phan du
	j set_first_number	#Chuyen den function chuyen 2 byte dang 2 led thanh so de tinh toan	
					
								
#Tinh so dau hien thi tren led

set_first_number:
	
	addi $s4, $t9, 0
	add $s6, $s4, $s5
	beqz $s6, sualoi
	li $t9, 0
	j done


#Truong hop phim f (=)

case_f:
	addi $s5, $t9, 0	


#Tinh so thu 2 hien thi tren den led 										

									
set_second_number:
	beq	$s3, 1, phep_cong	#s3 = 1 -> cong
	beq	$s3, 2, phep_tru	#s3 = 2 -> tru
	beq	$s3, 3, phep_nhan	#s3 = 3 -> nhan
	beq	$s3, 4, phep_chia	#s3 = 4 -> chia
	beq	$s3, 5, phep_lay_du	#s3 = 5 -> lay du
	
phep_cong:
	add	$s6, $s5, $s4
	li	$s3, 0
	li	$t9, 0
	j	in_phep_cong
	nop
	

#In ra dinh dang: a + b = c \n													
														
in_phep_cong:
	li	$v0, 1
	move	$a0, $s4
	syscall
	li	$s4, 0			#reset lai s4
	
	li	$v0, 11
	li	$a0, '+'
	syscall
												
	li	$v0, 1
	move	$a0, $s5
	syscall
	li	$s5, 0			#reset lai s5
																
	li	$v0, 11
	li	$a0, '='
	syscall
	
	li	$v0, 1
	move	$a0, $s6
	syscall
	nop
	
	li	$v0, 11
	li	$a0, '\n'
	syscall
	li	$s7, 100
	div	$s6, $s7
	mfhi	$s6			#Lay 2 gia tri cuoi cua ket qua
	j show_result_in_led
	nop
	
phep_tru:
	sub	$s6, $s4, $s5
	li	$s3, 0
	li	$t9, 0
	j	in_phep_tru
	nop


#In ra dinh dang: a - b = c \n													
																																												
in_phep_tru:
	li	$v0, 1
	move	$a0, $s4
	syscall
	li	$s4, 0			#reset s4
	
	li	$v0, 11
	li	$a0, '-'
	syscall
	
	li	$v0, 1
	move	$a0, $s5
	syscall
	li	$s5, 0			#reset s5
	
	
	li	$v0, 11
	li	$a0, '='
	syscall
	
	li	$v0, 1
	move	$a0, $s6
	syscall
	
	li	$v0, 11
	li	$a0, '\n'
	syscall
	
	li	$s7, 100
	div	$s6, $s7
	mfhi	$s6			#Lay 2 gia tri cuoi cua ket qua
	j show_result_in_led
	nop

phep_nhan:
	mul	$s6, $s4, $s5
	li	$s3, 0
	li	$t9, 0
	j	in_phep_nhan
	nop

	
#In ra dinh dang: a*b = c \n													
		
in_phep_nhan:
	li	$v0, 1
	move	$a0, $s4
	syscall
	li	$s4, 0			#reset s4
	
	li	$v0, 11
	li	$a0, '*'
	syscall
	
	li	$v0, 1
	move	$a0, $s5
	syscall
	li	$s5, 0			#reset s5
	
	
	li	$v0, 11
	li	$a0, '='
	syscall
	
	li	$v0, 1
	move	$a0, $s6
	syscall
	
	li	$v0, 11
	li	$a0, '\n'
	syscall
	
	li	$s7, 100
	div	$s6, $s7
	mfhi	$s6			#Lay 2 gia tri cuoi cua ket qua
	j show_result_in_led
	nop																											
																																														
phep_chia:
 	beq	$s5, 0, chia_0		#Kiem tra xem co phai so chia = 0 hay ko
 	li	$s3, 0
 	div	$s4, $s5
 	mflo	$s6
 	mfhi	$s7
 	li	$t9, 0
 	j	in_phep_chia
 	nop
 	

#Neu $s5 = 0 in ra: "Khong the chia cho so 0 \n"												

chia_0:
	li	$v0, 55
	la	$a0, mess1
	syscall
	li	$s6, 0
	j	show_result_in_led
	
in_phep_chia:
	li	$v0, 1
	move	$a0, $s4
	syscall
	li	$s4, 0			#reset s4
	
	li	$v0, 11
	li	$a0, '/'
	syscall
	
	li	$v0, 1
	move	$a0, $s5
	syscall
	li	$s5, 0			#reset s5
	
	
	li	$v0, 11
	li	$a0, '='
	syscall
	
	li	$v0, 1
	move	$a0, $s6
	syscall
	
	
	li	$v0, 11
	li	$a0, '\n'
	syscall
	
	li	$s7, 100
	div	$s6, $s7
	mfhi	$s6			#Lay 2 gia tri cuoi cua ket qua
	j show_result_in_led
	nop
 
 
 phep_lay_du:
 	beq	$s5, 0, chia_0	#Kiem tra xem so chia co = 0 hay ko
 	li	$s3, 0
 	div	$s4, $s5
 	mflo	$s7
 	mfhi	$s6
 	li	$t9, 0
 	j	in_phep_lay_du
 	nop
 	

#Neu $s5 = 0 in ra: "Khong the chia cho so 0 \n"												

	
in_phep_lay_du:
	li	$v0, 1
	move	$a0, $s4
	syscall
	li	$s4, 0			#reset s4
	
	li	$v0, 11
	li	$a0, '%'
	syscall
	
	li	$v0, 1
	move	$a0, $s5
	syscall
	li	$s5, 0			#reset s5
	
	li	$v0, 11
	li	$a0, '='
	syscall
	
	li	$v0, 1
	move	$a0, $s6
	syscall
	
	
	li	$v0, 11
	li	$a0, '\n'
	syscall
	
	li	$s7, 100
	div	$s6, $s7
	mfhi	$s6			#Lay 2 gia tri cuoi cua ket qua
	j show_result_in_led
	nop
 
#------------------------------------	
#Hien thi ket qua tren den led
#So 'ab'
#Den trai = a = ab div 10
#Den phai = b = ab mod 10
#------------------------------------	
show_result_in_led:
	li	$t8, 10			#Gia tri trung gian = 10
	div	$s6, $t8		#$s6 = a
	mflo	$t7			#$t7 = result
	jal 	check			#Chuyen den ham chuyen t7 -> bit hien thi tren led
	
	sb	$t4, 0($t0)		#Hien thi tren led trai
	
 	add	$s2, $t7, $zero		#s1 = gia tri bit led phai
 	
 	mfhi	$t7			#t7 = phan du con lai
	jal	check			#convert t7 thanh bit hien thi tren led
	sb	$t4, 0($t5)		#Hien thi len led phai
	
	add	$s1, $t7, $zero		#s1 = gia tri bit led phai
	j	reset_led		#ham reset lai led
 	
 check:
 	addi	$sp, $sp, 4
 	sw	$ra, 0($sp)
 	beq	$t7, 0, check_0		#t7 = 0 -> Hien thi so 0 tren thanh led																																																																																																																									
 	beq	$t7, 1, check_1		#t7 = 1 
 	beq	$t7, 2, check_2		#t7 = 2 
 	beq	$t7, 3, check_3		#t7 = 3 
 	beq	$t7, 4, check_4		#t7 = 4 
 	beq	$t7, 5, check_5		#t7 = 5 	
 	beq	$t7, 6, check_6		#t7 = 6 
 	beq	$t7, 7, check_7		#t7 = 7 
 	beq	$t7, 8, check_8		#t7 = 8 
 	beq	$t7, 9, check_9		#t7 = 9 																																																																																																																																																																																																																																																		

#Chuyen gia tri thanh bit hien thi tren thanh led																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																				
check_0:
	lb	$t4, zero
	j	finish_check
check_1:
	lb	$t4, one
	j	finish_check
check_2:
	lb	$t4, two
	j	finish_check
check_3:
	lb	$t4, three
	j	finish_check
check_4:
	lb	$t4, four
	j	finish_check
check_5:
	lb	$t4, five
	j	finish_check
check_6:
	lb	$t4, six
	j	finish_check
check_7:
	lb	$t4, seven
	j	finish_check
check_8:
	lb	$t4, eight
	j	finish_check
check_9:
	lb	$t4, nine
	j	finish_check

finish_check:
	lw	$ra, 0($sp)
	addi	$sp, $sp, -4
	jr	$ra
	
update:
	mul	$t9, $t9, 10
	add	$t9, $t9, $t7


#Hoan thanh xong 1 so -> reset_led		

done:
	beq $s0,1, reset_led
	nop


#Function hien thi bit len led ben trai

load_to_left_led:
	lb	$t6, 0($sp)		#Load bit hien thi led tu stack
	add	$sp, $sp, -4
	lb	$t8, 0($sp)		#Load gia tri cua bit nay
	add	$sp, $sp, -4
	add	$s2, $t8, $zero		#s2 = gia tri bit led trai
	sb	$t6, 0($t0)		#Hien thi len led trai 


#Function hien thi bit len led ben phai

load_to_right_led:
	sb	$t4, 0($t5)		#Hien thi bit len led phai
	add	$sp, $sp, 4	
	sb	$t7, 0($sp)		#Day gia tri bit nay vao stack
	add	$sp, $sp, 4
	sb	$t4, 0($sp)		#Day bit nay vao stack
	add	$s1, $t7, $zero		#s1 = gia tri bit led phai
	j 	return


reset_led:
	li	$s0, 0			#s0 = 0 ->doi nhap so tiep theo trong 2 so
	li	$t8, 0
	addi	$sp, $sp, 4
	sb	$t8, 0($sp)
	lb	$t6, zero		#day bit zero vao stack
	addi	$sp, $sp, 4
	sb	$t6, 0($sp)

	
return:
	la	$a3, Loop1
	mtc0, 	$a3, $14
	eret

	
sualoi:	
li	$v0, 55
	la	$a0, mess2
	syscall
